				float2 temp_cast_0 = (_windSpeed).xx;
				float3 ase_worldPos = TransformObjectToWorld( (v.vertex).xyz );
				float2 appendResult18 = (float2(ase_worldPos.x , ase_worldPos.y));
				float2 panner21 = ( 1.0 * _Time.y * temp_cast_0 + ( appendResult18 * _windTiling ));
				float4 tex2DNode20 = tex2Dlod( _NoisePerlinWindAndColor, float4( panner21, 0, 0.0) );
				float3 break30 = _windDirection;
				float3 appendResult35 = (float3(( break30.x * -1.0 ) , ( break30.y * -1.0 ) , ( break30.z * -1.0 )));
				float3 lerpResult26 = lerp( _windDirection , appendResult35 , tex2DNode20.r);
				
				o.ase_texcoord8.xy = v.texcoord.xy;
				o.ase_color = v.ase_color;
				
				//setting value to unused interpolator channels and avoid initialization warnings
				o.ase_texcoord8.zw = 0;

